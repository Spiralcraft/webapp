<scm:Change.list
  xmlns:scm="class:/spiralcraft/scm/meta/"
  >
  
  <scm:Change>
  
    <revision>0.4.16
    </revision>

    <authorId>miketoth
    </authorId>
    
    <date>
    </date>

    <detail>
      <scm:Change>

        <revision>-dev2
        </revision>
                
        <scm:Notes>
          <scm:Note>page.tgl has a hook for custom css (dynamic:/spiralcraft/webapp/custom-css)
          </scm:Note>
        </scm:Notes> 

        <path>package/war-webui/page.tgl
        </path>
        
      </scm:Change>

      <scm:Change>

        <revision>-dev2
        </revision>
                
        <scm:Notes>
          <scm:Note>Provide data-based view resolution (incomplete)
          </scm:Note>
        </scm:Notes> 

        <path>package
        </path>
        
        <resources>
          <URI>config/NavFilter.assy.xml
          </URI>
          <URI>war-classes/spiralcraft/webapp/meta/Path.type.xml
          </URI>
        </resources>
        
      </scm:Change>

      <scm:Change>

        <revision>-dev2
        </revision>
                
        <scm:Notes>
          <scm:Note>Move to extension-less (no .webui) URI paths using the
            UIServlet as the default servlet, permitting any static
            resource to be served by a dynamic .webui file.
          </scm:Note>
        </scm:Notes> 

        <path>package
        </path>
        
        <resources>
          <URI>config/AdminNav.paths.xml
          </URI>
          <URI>config/Context.assy.xml
          </URI>
          <URI>war-webui/function/login/authMenu.tgl
          </URI>
        </resources>
        
      </scm:Change>

      <scm:Change>

        <revision>-dev1
        </revision>
                
        <scm:Notes>
          <scm:Note>Add random password generator to registration context
          </scm:Note>
        </scm:Notes> 

        <path>package/war-webui/function/registration/context.tgl
        </path>
        
      </scm:Change>

      <scm:Change>

        <revision>-dev1
        </revision>
                
        <scm:Notes>
          <scm:Note>Add base server Context.assy.xml
          </scm:Note>
        </scm:Notes> 

        <path>package/config/Context.assy.xml
        </path>
        
        
      </scm:Change>

      <scm:Change>

        <revision>-dev1
        </revision>
                
        <scm:Notes>
          <scm:Note>Add css classes for data tables
          </scm:Note>
        </scm:Notes> 

        <path>package/war-css/themes/basic/style.css
        </path>
        
        
      </scm:Change>
    </detail>
  </scm:Change>
  

  <scm:Change>
  
    <revision>0.4.15.1
    </revision>

    <authorId>miketoth
    </authorId>
    
    <date>2011-09-15
    </date>

    <detail>
      <scm:Change>
        <revision>-dev4
        </revision>
        
        <scm:Notes>
          <scm:Note>Add standard set of Admin nav paths
          </scm:Note>
        </scm:Notes> 

        <path>package/config/AdminNav.paths.xml
        </path>
        
        
      </scm:Change>

      <scm:Change>
        <revision>-dev4
        </revision>
        
        <scm:Notes>
          <scm:Note>Add identity role mapping data support
          </scm:Note>
        </scm:Notes> 

        <path>package
        </path>
        
        <resources>
          <URI>config/SecurityFilter.assy.xml
          </URI>
          <URI>war-classes/webapp/meta/ActorRole.type.xml
          </URI>
          <URI>war-classes/webapp/meta/Identity.type.xml
          </URI>
          <URI>war-classes/webapp/meta/WebApp.schema.xml
          </URI>
        </resources>
        
      </scm:Change>
      
      <scm:Change>
        <revision>-dev4
        </revision>
        
        <scm:Notes>
          <scm:Note>Add config differentiated SMTP service to global config
          </scm:Note>
        </scm:Notes> 

        <path>package/config
        </path>
        
        <resources>
          <URI>global.control.xml
          </URI>
          <URI>SMTPConnector.example.xml
          </URI>
          <URI>SMTPFilter.assy.xml
          </URI>
        </resources>
        
      </scm:Change>

      <scm:Change>
        <revision>-dev4
        </revision>
        
        <scm:Notes>
          <scm:Note>Support multiple types of forms for each function
          </scm:Note>
        </scm:Notes> 

        <path>package/war-webui/function/
        </path>
        
        <resources>
          <URI>login/loginForm.tgl
          </URI>
          <URI>login/form.tgl
          </URI>
          <URI>registration/changePasswordForm.tgl
          </URI>
          <URI>registration/registrationForm.tgl
          </URI>
          <URI>registration/form.tgl
          </URI>
        </resources>
        
      </scm:Change>

      <scm:Change>
        <revision>-dev3
        </revision>
        
        <scm:Notes>
          <scm:Note>Support additional contexts for editing registration data
            and passwords.
          </scm:Note>
        </scm:Notes> 
        
        <path>package/war-webui/function/registration
        </path>
        
        <resources>
          <URI>LoginEditor.assy.xml
          </URI>
          <URI>MyLoginEditor.assy.xml
          </URI>
          <URI>UsernameHiddenInput.assy.xml
          </URI>
        </resources>
      </scm:Change>

      <scm:Change>
        <revision>-dev2
        </revision>
        
        <scm:Notes>
          <scm:Note>Promoted NavContext mechanism to the component level in
            spiralcraft-core.
          </scm:Note>
        </scm:Notes> 
        
        <path>package
        </path>
        
        <resources>
          <URI>war-classes/spiralcraft/webapp/meta/NavContext.type.xml
          </URI>
          <URI>war-webui/themes/basic/navBar.tgl
          </URI>
          <URI>war-webui/themes/basic/page.tgl
          </URI>
        </resources>
      </scm:Change>

      <scm:Change>
        <revision>-dev2
        </revision>
        
        <scm:Notes>
          <scm:Note>Rename NavigationContext to NavContext
          </scm:Note>
        </scm:Notes> 
        
        <path>package
        </path>
        
        <resources>
          <URI>war-classes/spiralcraft/webapp/meta/NavContext.type.xml
          </URI>
          <URI>war-classes/spiralcraft/webapp/meta/NavigationContext.type.xml
          </URI>
          <URI>war-webui/themes/basic/navBar.tgl
          </URI>
          <URI>war-webui/themes/basic/page.tgl
          </URI>
        </resources>
      </scm:Change>

      <scm:Change>
        <revision>-dev1
        </revision>
        
        <scm:Notes>
          <scm:Note>Data driven nav model
          </scm:Note>
          <scm:Note>Minimalist nav tab layout
          </scm:Note>
        </scm:Notes> 
        
        <path>package
        </path>
        
        <resources>
          <URI>war-classes/spiralcraft/webapp/meta/NavigationContext.type.xml
          </URI>
          <URI>war-classes/spiralcraft/webapp/meta/Path.type.xml
          </URI>
          <URI>war-classes/spiralcraft/webapp/meta/Place.type.xml
          </URI>
          <URI>war-classes/spiralcraft/webapp/meta/WepApp.schema.xml
          </URI>
          <URI>war-css/themes/basic/layout.css
          </URI>
          <URI>war-css/themes/basic/navigation.css
          </URI>
          <URI>war-webui/layout/navBar.stub.tgl
          </URI>
          <URI>war-webui/layout/navBar.tgl
          </URI>
          <URI>war-webui/themes/basic/navBar.tgl
          </URI>
          <URI>war-webui/themes/basic/page.tgl
          </URI>
          <URI>war-webui/themes/basic/topBar.tgl
          </URI>
        </resources>
      </scm:Change>

      <scm:Change>
        <revision>-dev1
        </revision>
        
        <scm:Notes>
          <scm:Note>Development of basic theme
          </scm:Note>
          <scm:Note>Formalization of css selector naming
          </scm:Note>
        </scm:Notes> 
        
        <path>package
        </path>
        
        <resources>
          <URI>war-css/themes/basic/layout.css
          </URI>
          <URI>war-css/themes/basic/style.css
          </URI>
          <URI>war-css/themes/basic/topbar.css
          </URI>
          <URI>war-webui/themes/basic/contentFull.tgl
          </URI>
          <URI>war-webui/themes/basic/footer.tgl
          </URI>
          <URI>war-webui/themes/basic/header.tgl
          </URI>
          <URI>war-webui/themes/basic/page.tgl
          </URI>
          <URI>war-webui/themes/basic/panelFooter.tgl
          </URI>
          <URI>war-webui/themes/basic/panelHeader.tgl
          </URI>
          <URI>war-webui/themes/basic/taskPage.tgl
          </URI>
          <URI>war-webui/themes/basic/taskPanel.tgl
          </URI>
          <URI>war-webui/themes/basic/topBar.tgl
          </URI>
          <URI>war-webui/page.tgl
          </URI>
        </resources>

      </scm:Change>
    </detail>
  </scm:Change>
</scm:Change.list>