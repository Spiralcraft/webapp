<af:PathContext
  xmlns:af="class:/spiralcraft/servlet/autofilter/"
  xmlns:code="context://code/"
  xmlns:swa-code="context://code/spiralcraft-webapp/"  
  xmlns:swa-config="context://code/spiralcraft-webapp/config/"  
  >

  <codeBaseURI>bundle://spiralcraft-webapp.war-webui/
  </codeBaseURI>
  
  <contentBaseURI>bundle://spiralcraft-webapp.war-webui/content/
  </contentBaseURI>

  <placeContext>
    <swa-code:Place/>
  </placeContext>
  
  <filters
    xmlns:smtp="class:/spiralcraft/net/smtp/"
    xmlns:webui="class:/spiralcraft/servlet/webui/"
    xmlns:config="context://config/"
    xmlns:webapp="context://code"
    >
  
    <swa-config:SMTPFilter/>
  
    <swa-config:SecurityFilter/>
 
    <swa-config:MainNavFilter/>
    
  </filters>
  
</af:PathContext>